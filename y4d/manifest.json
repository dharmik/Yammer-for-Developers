{
  "manifest_version": 2,

  "name"       : "Yammer for Developers",
  "description": "The code formater and highlighter in Yammer's messages.",
  "icons"      : {"128": "icon_128.png"},
  "version"    : "0.2.0",

  "content_scripts": [
    {
      "matches": ["https://www.yammer.com/*"],
      "css"    : ["vs.css", "y4d.css"],
      "js"     : ["highlight.pack.js", "ContentFormatter.js", "PostFormatter.js", "y4d.js"]
    }
  ]
}
